{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node LTS (22)",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "noUncheckedIndexedAccess": true
  },
  "include": ["src"]
}
